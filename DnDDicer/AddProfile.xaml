<Window x:Class="DnDDicer.AddProfile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DnDDicer"
        mc:Ignorable="d"
        Title="D&amp;D Dicer Profile Add" Height="150" Width="300">
	<Grid>
		<Button x:Name="okBtn" Content="Add" HorizontalAlignment="Center"  Margin="0,64,0,0" VerticalAlignment="Top" Width="74" Click="okBtn_Click"/>
		<TextBox x:Name="profTxt" HorizontalAlignment="Center" Height="23" Margin="0,24,0,0" TextWrapping="Wrap" Text="Profile Name" VerticalAlignment="Top" Width="120" KeyUp="profTxt_KeyUp"/>

	</Grid>
</Window>
